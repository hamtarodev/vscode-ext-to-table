{
	"name": "ToTable",
	"displayName": "ToTable",
	"description": "Convert / View CSV to Table on the fly",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ToTable.viewTable"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ToTable.viewTable",
				"title": "View CSV as Table"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"css-loader": "^6.6.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-error-overlay": "^6.0.10",
		"style-loader": "^3.3.1"
	},
	"resolutions": {
		"react-error-overlay": "^6.0.10"
	}
}
